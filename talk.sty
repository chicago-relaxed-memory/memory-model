\newcommand{\nohighlight}[2][]{#1}
\newcommand{\highlight}[2][yellow]{\ifmmode%
  \smash{\mathchoice%
  {\colorbox{#1}{{$\mathstrut\displaystyle#2$}}}%
  {\colorbox{#1}{{$\mathstrut\textstyle#2$}}}%
  {\colorbox{#1}{{$\mathstrut\scriptstyle#2$}}}%
  {\colorbox{#1}{{$\mathstrut\scriptscriptstyle#2$}}}}%
  \else%
    \colorbox{#1}{#2}%
  \fi}
\newcommand{\nohightight}[2][]{{\setlength{\fboxsep}{0pt}{#2}}}
\newcommand{\hightight}[2][yellow]{{\setlength{\fboxsep}{0pt}\highlight[#1]{#2}}}
\newcommand{\bggray}[2][gray!20]{\smash{\mathchoice%
  {\colorbox{#1}{\smash{$\displaystyle#2$}}}%
  {\colorbox{#1}{\smash{$\textstyle#2$}}}%
  {\colorbox{#1}{$\scriptstyle#2$}}%
  {\colorbox{#1}{$\scriptscriptstyle#2$}}}}%

